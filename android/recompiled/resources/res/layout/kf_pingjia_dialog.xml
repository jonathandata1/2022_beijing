<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/direct_seeding_linear" android:background="@drawable/kf_calling_linear" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="18sp" android:id="@+id/direct_seeding" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="15dp" android:layout_marginBottom="15dp" android:text="非常满意"/>
    </LinearLayout>
    <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/call_return_linear" android:background="@drawable/kf_calling_linear" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="18sp" android:id="@+id/call_return" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="15dp" android:layout_marginBottom="15dp" android:text="满意"/>
    </LinearLayout>
    <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/ordinary_call_linear" android:background="@drawable/kf_calling_linear" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="18sp" android:id="@+id/ordinary_call" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="15dp" android:layout_marginBottom="15dp" android:text="不满意"/>
    </LinearLayout>
    <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/cancel_linear" android:background="@drawable/kf_calling_linear" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="18sp" android:id="@+id/cancel" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="15dp" android:layout_marginBottom="15dp" android:text="取消"/>
    </LinearLayout>
</LinearLayout>
