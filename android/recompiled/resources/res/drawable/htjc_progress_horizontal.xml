<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient android:startColor="#164d54" android:endColor="#164d54" android:angle="270" android:centerY="0.75" android:centerColor="#164d54"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#1ac6b4" android:endColor="#1ac6b4" android:angle="270" android:centerY="0.75" android:centerColor="#1ac6b4"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#1ac6b4" android:endColor="#1ac6b4" android:angle="270" android:centerY="0.75" android:centerColor="#1ac6b4"/>
            </shape>
        </clip>
    </item>
</layer-list>
